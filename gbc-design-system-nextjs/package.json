{
    "name": "@guilhermebc/gbc-design-system-nextjs",
    "version": "1.0.0",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
      ".": {
        "import": "./src/index.js",
        "types": "./src/index.d.ts"
