      "tsc": "tsc -p . --outDir ./dist"
    },
    "dependencies": {
      "@guilhermebc/gbc-design-system-core": "^0.0.1",
      "@stencil/react-output-target": "^0.7.1"
    },
    "devDependencies": {
      "@types/node": "^20",
      "@types/react": "^18",
      "react": "^18",
      "react-dom": "^18",
      "typescript": "^5"
    }
  }